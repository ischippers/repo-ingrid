package nl.exampleCompany.carcase;

import nl.exampleCompany.carcase.domain.Car;

public class CarApp {
    public static void main(String[] args) {
        int  speed = 0, age = 0, yearOfRegistration = 1980;
        System.out.println("Welcome to my very first car application! Yahoo!");
        System.out.println("speed = " + speed ) ;

        Car auto1 = new Car (132, 25000.00, "AA-BB-CC");

        System.out.println(auto1);

        Car auto2 = new Car ();

        auto2.setSpeed(98);
        auto2.setPrice(12000.00);
        auto2.setLicencePlate( "JA-VA-08");
        System.out.println(auto2);
    }
}


